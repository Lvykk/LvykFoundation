{
  "name": "LvykFoundation",
  "version": "1.0.0",
  "summary": "A short description of LvykFoundation.",
  "description": "A tool class for secondary packaging of network and cache libraries",
  "homepage": "https://github.com/Lvykk/LvykFoundation",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Lvyk": "lvyk1029@gmail.com"
  },
  "source": {
    "git": "https://github.com/Lvykk/LvykFoundation.git",
    "tag": "1.0.0"
  },
  "platforms": {
    "ios": "11.0"
  },
  "pod_target_xcconfig": {
    "VALID_ARCHS": "x86_64 armv7 arm64"
  },
  "requires_arc": true,
  "ios": {
    "frameworks": "UIKit"
  },
  "dependencies": {
    "Moya": [
      "~> 15.0.0"
    ],
    "BigInt": [
      "~> 5.2.0"
    ],
    "Cache": [
      "~> 6.0.0"
    ]
  },
  "subspecs": [
    {
      "name": "Cache",
      "source_files": "LvykFoundation/Classes/Cache/**/*"
    },
    {
      "name": "Decodable",
      "source_files": "LvykFoundation/Classes/Decodable/**/*"
    },
    {
      "name": "Effects",
      "source_files": "LvykFoundation/Classes/Effects/**/*",
      "ios": {
        "frameworks": "Accelerate"
      }
    },
    {
      "name": "Macro",
      "source_files": "LvykFoundation/Classes/Macro/**/*"
    },
    {
      "name": "Network",
      "source_files": "LvykFoundation/Classes/Network/**/*"
    },
    {
      "name": "NSDecimalNumber",
      "source_files": "LvykFoundation/Classes/NSDecimalNumber/**/*"
    }
  ]
}
